CREATE database GERENCIAMENTO;
USE GERENCIAMENTO;
 
 CREATE TABLE DADOSCLIENTES(
 ID INT PRIMARY KEY AUTO_INCREMENT,
 CLIENTE varchar(30) NOT NULL,
 DESCPROD varchar(100) NOT NULL,
 DATAVENDA date NOT NULL,
 VALORPROD decimal NOT NULL
 );

 CREATE TABLE CONTROLEESTOQUE(
 ID INT PRIMARY KEY AUTO_INCREMENT,
 DESCPROD varchar(100) NOT NULL,
 QUANTIDADE INT NOT NULL,
 DATAESTOQUE date NOT NULL,
 VALORESTOQUE decimal NOT NULL
 );

select*FROM DADOSCLIENTES;

insert INTO DADOSCLIENTES(ID, CLIENTE, DESCPROD, DATAVENDA, VALORPROD)
values(1, "MARIA", "PULSEIRAROSA", '23/11/02', 10);
